#include <stdio.h>
int main()
{
    int i,j,n=5,k,m;
    printf("Enter the number of rows: ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
         for(m=0;m<n-i;m++){
                printf(" ");
            }
        for(j=0;j<n;j++)
        {
           
            printf("*");
    }
    printf("\n");
}

printf("\n");
return 0;
}
